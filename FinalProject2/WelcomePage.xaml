<Window x:Class="FinalProject2.WelcomePage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FinalProject2"
        mc:Ignorable="d"
        Title="WelcomePage" Height="450" Width="300">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="pokeball.jpg" Stretch="UniformToFill"/>
        </Grid.Background>
        <Label Content="WELCOME TO THE POKEMON STORE" Margin="10,78,0,0" VerticalAlignment="Top" Height="46" FontSize="16" HorizontalAlignment="Left" Width="274"/>
        <Button Content="SignUp" HorizontalAlignment="Left" Margin="10,334,0,0" VerticalAlignment="Top" Width="99" Click="Button_Click"/>
        <Button Content="LogIn" HorizontalAlignment="Left" Margin="175,334,0,0" VerticalAlignment="Top" Width="95" Click="Button_Click_1"/>

    </Grid>
</Window>
